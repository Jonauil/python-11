lst = [1,1]
i = 1
while True:
    i +=1
    if lst[i-2] + lst[i-1] > 100:
        break
    lst.append(lst[i-2] + lst[i-1])
    print(lst)

lst = [1,1]
a=0
for i in range(2,102):
    lst.append(lst[i-1]+lst[i-2])
    if i == 101:
        a=lst[i-1] + lst[i-2]
print(a)

